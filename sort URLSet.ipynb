   "source": [
    "f = open(\"PostUrlSet2.txt\", 'a',encoding='UTF-8') # 파일 쓰기를 위한 f 변수 할당 #\n",
    "\n",
    "with open ('PostUrlSet.txt', 'r') as fr:\n",
    "    Post_list = []\n",
    "    for line in fr:\n",
    "        Post_list.append(line)\n",
    "    my_set = set(Post_list)\n",
    "    Post_Url_List = list(my_set)\n",
    "    f.write(''.join(Post_Url_List))"
   ]
